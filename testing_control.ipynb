   "outputs": [],
   "source": [
    "# Прописать название опроса из Приложения\n",
    "\n",
    "survey_name = 'Название опроса'\n",
    "survey_id = all_polls.loc[all_polls['name'] == survey_name, 'id'].reset_index()['id'][0]"
   ]
  },
  {
   "cell_type": "code",
