   "outputs": [],
   "source": [
    "survey_filt_2 = survey_filt[['id', 'answer_date', 'sent_date', 'status', 'answers', 'poll_fact', 'poll_max']]\n",
    "exploded_df = survey_filt_2.explode('answers', ignore_index=True)\n",
    "m_df = pd.concat([exploded_df.drop('answers', axis=1), pd.json_normalize(exploded_df['answers'])], axis=1)\n",
    "df_to_open = m_df.loc[m_df['answer'].isna() == False]"
   ]
  },
  {
   "cell_type": "code",
